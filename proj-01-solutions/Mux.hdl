/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Xor(a=a, b=sel, out=xora);
    And(a=a, b=xora, out=alpha);
    Not(in=sel, out=notsel);
    Xor(a=b, b=notsel, out=xorb);
    And(a=b, b=xorb, out=beta);
    Or(a=alpha, b=beta, out=out);
}